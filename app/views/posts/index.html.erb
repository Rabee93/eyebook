
<h1>Current properties</h1>

<table>
  <tr>
    <th>user id</th>
    <th>bedroom</th>
    <th>location</th>
    <th>type</th>
    <th>comments</th>
    <th>image</th>
    <th colspan="3"></th>
  </tr>

  <% @posts.each do |p| %>
    <tr>
      <td><%= p.user.id %></td>
      <td><%= p.bedroom %></td>
      <td><%= p.city %></td>
      <td><%= p.property_type %></td>
      <td><%= p.comments%></td>
      <td><img height="100" src="<%= p.picture%>"></img></td>
    </tr>
  <% end %>
</table>
