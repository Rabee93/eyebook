<%= form_with model: @post, local: true do |form| %>
  <p>
    <%= form.label :type %><br>
    <%= form.select :property_type, ['house', 'flat', 'shared property']%>
  </p>
<p>
  <%= form.label :bedroom %><br>
    <%= form.select :bedroom, [1, 2, 3, 4]%>
    </p>
    <p>
      <%= form.label :city %><br>
      <%= form.select :city, ['London', 'Manchester', 'Birmingham','Leeds']%>
   </p>
   <p>
     <%= form.label :price_per_month %><br>
     <%= form.text_area :price %>
   </p>
  <p>
    <%= form.label :description %><br>
    <%= form.text_area :comments %>
  </p>
  <p>
    <%= form.label :images %><br>
     <%= form.text_area :picture %>
  </p>
  <p>
    <%= form.submit %>
  </p>
  <img src= "https://www.hometree.co.uk/img/768/53abfdd5ac160a6843995524f44b4920why-you-need-locks-and-keys-insurance-transparent.png">
<% end %>
